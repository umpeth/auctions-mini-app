{
  "name": "my-minikit-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "format": "prettier --check --ignore-unknown $(git ls-files -m --others --exclude-standard)",
    "format:fix": "prettier --write --ignore-unknown $(git ls-files -m --others --exclude-standard)",
    "generate-graphql": "graphql-codegen --config codegen.yml && prettier --write graphql/generated.ts",
    "wagmi": "wagmi generate && pnpm format:fix"
  },
  "lint-staged": {
    "!(*md)": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@coinbase/onchainkit": "latest",
    "@farcaster/frame-sdk": "^0.0.35",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-toast": "^1.2.13",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@tanstack/react-query": "^5",
    "@upstash/redis": "^1.34.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "is-ipfs": "^8.0.4",
    "lucide-react": "^0.507.0",
    "next": "^14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.27.2",
    "wagmi": "^2.14.11"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^3.0.7",
    "@graphql-codegen/cli": "^5.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@wagmi/cli": "^2.2.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
